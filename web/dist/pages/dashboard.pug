html
    head
        title CodeComp | #{competition.name} Dashboard
        include _mixins/header
    body
        include _mixins/nav-cent
        .container
            br
            h3=competition.name
            div
                a(href='challenges/new') Create Challenge
            h5 Challenges
            if competition.challenges.length
                for challenge in competition.challenges
                    div=challenge
            else
                i No challenges available
